<%# form_with url: "/users/register", model: @user, method: :post, local: true do |f| %>
<%= form_with url: "/users/register", method: :post, local: true do |f| %>
<%= f.label :name %>
<%= f.text_field :name %>
<%= f.label :email %>
<%= f.text_field :email %>
<%= f.label :password, 'Password' %>
<%= f.text_field :password %>
<%= f.label :password_confirmation, "Password Confirmation" %>
<%= f.text_field :password_confirmation %>
<%= f.submit 'Register' %>
<% end %>
